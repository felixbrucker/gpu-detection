{
  "name": "gpu-detection",
  "version": "0.1.4",
  "description": "",
  "main": "lib/index.js",
  "author": "Felix Brucker",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felixbrucker/gpu-detection.git"
  },
  "dependencies": {
    "neon-cli": "^0.7.1",
    "node-pre-gyp": "^0.17.0"
  },
  "scripts": {
    "install": "npm run --silent install:download || npm run --silent build || exit 0",
    "install:download": "node-pre-gyp install --fallback-to-build=false",
    "build": "neon build --release && node ./rename-native-module.js",
    "package": "yarn run build && node ./build-package.js"
  },
  "devDependencies": {
    "archiver": "^5.2.0",
    "mkdirp": "^1.0.4"
  },
  "binary": {
    "module_name": "gpu-detection",
    "module_path": "./native/",
    "remote_path": "{version}",
    "package_name": "{platform}-{arch}.tar.gz",
    "host": "https://github.com/felixbrucker/gpu-detection/releases/download/",
    "pkg_path": "."
  },
  "engines": {
    "node": ">= 10"
  }
}
